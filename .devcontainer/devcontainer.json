{
  "name": "Svelte Environment",
  "image": "ueber/svelte-environment:0.0.1",
  "customizations": {
    "vscode": {
      "settings": {
        "chat.disableAIFeatures": true,
        "chat.agent.enabled": false,
        "workbench.editorAssociations": {
          "*.md": "vscode.markdown.preview.editor"
        },
        "terminal.integrated.shell.linux": "/bin/bash",
        "files.exclude": {
          ".git/": true,
          ".gititnore": true,
          ".devcontainer/": true,
          "docker/": true,
          "makefile": true
        }
      }
    }
  },
  "remoteUser": "root"
}